{
  "name": "Power Time Tracking",
  "productName": "PowerTimeTracking",
  "version": "0.7.0",
  "description": "An app to improve productivity by blocking apps that waste time and showing where time goes.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start DEBUGMENU=1",
    "package": "electron-forge package",
    "package-arm": "electron-forge package --arch=arm64",
    "make": "electron-forge make",
    "make-arm": "electron-forge make --arch=arm64",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Shorya Malani",
    "email": "shoryamal@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "@electron/osx-sign": "^1.0.1",
    "apexcharts": "^3.35.3",
    "dotenv": "^16.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "node-fetch": "^2.6.7",
    "purecss": "^2.1.0",
    "python-shell": "^3.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^20.1.0",
    "electron-notarize": "^1.2.1"
  },
  "build": {
    "extraResources": [
      "py",
      {
        "from": "python",
        "to": "python",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
